<!DOCTYPE html>
<html ng-app="SwiftSearch">
<head>
  <title><%= yield(:title) %></title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="header">
    <%= render 'layouts/shared/navigation' %>
  </div>
  <div class="container">
    <%= render 'layouts/shared/messages' %>
    <%= yield %>
  </div>
</body>
</html>
